<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net45</TargetFramework>
    <Authors>Gordon Sulc</Authors>
    <PackageProjectUrl>https://github.com/gsulc/NConfig</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/gsulc/NConfig/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/gsulc/NConfig</RepositoryUrl>
    <PackageTags>ini;configuration;config;poco;serialization;deserialization</PackageTags>
    <PackageReleaseNotes>beta</PackageReleaseNotes>
    <Version>0.5.4</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>A simple, lightweight serialization facade for reading and writing INI files to and from plain-old csharp objects.</Description>
    <Copyright>Gordon Sulc</Copyright>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>NConfigIniKey.snk</AssemblyOriginatorKeyFile>
    <DelaySign>false</DelaySign>
    <AssemblyVersion>0.5.4.0</AssemblyVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="ini-parser" Version="2.5.2" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\NConfig.Abstractions\NConfig.Abstractions.csproj" />
  </ItemGroup>

</Project>
