<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net20;netstandard1.3;netstandard2.0</TargetFrameworks>
    <PackageLicenseUrl>https://github.com/gsulc/NConfig/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/gsulc/NConfig</PackageProjectUrl>
    <RepositoryUrl>https://github.com/gsulc/NConfig</RepositoryUrl>
    <PackageTags>configuration;config;serialization;deserialization</PackageTags>
    <Authors>Gordon Sulc</Authors>
    <Version>0.5.2</Version>
    <PackageReleaseNotes>descriptive exceptions</PackageReleaseNotes>
    <Description>Reduces dependencies.</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Copyright>Gordon Sulc</Copyright>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>NConfigKey.snk</AssemblyOriginatorKeyFile>
    <DelaySign>false</DelaySign>
    <RootNamespace>NConfig</RootNamespace>
  </PropertyGroup>

</Project>
